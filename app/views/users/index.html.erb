<div class="container">
  <div class="row">
<%= render partial: 'books/infonew', locals: { user: @user, book: @book } %>
    <div class="col-md-8 offset-md-1">
      <div>
        <h2>Users</h2>
        <table class="table">
          <thead>
            <tr>
              <th></th>
              <th>image</th>
              <th>name</th>
            </tr>
          </thead>
          <tbody>
            <% @users.each do |user| %>
            <tr>
              <td><%= image_tag user.get_profile_image(80,80) %></td>
              <td><%= user.name %></td>
              <td><%= link_to "Show", user_path(user.id) %></td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>